#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/aicli-web/node_modules/.pnpm/vite@7.0.5_@types+node@22.16.5_jiti@2.4.2_sass@1.89.2/node_modules/vite/bin/node_modules:/workspace/aicli-web/node_modules/.pnpm/vite@7.0.5_@types+node@22.16.5_jiti@2.4.2_sass@1.89.2/node_modules/vite/node_modules:/workspace/aicli-web/node_modules/.pnpm/vite@7.0.5_@types+node@22.16.5_jiti@2.4.2_sass@1.89.2/node_modules:/workspace/aicli-web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/aicli-web/node_modules/.pnpm/vite@7.0.5_@types+node@22.16.5_jiti@2.4.2_sass@1.89.2/node_modules/vite/bin/node_modules:/workspace/aicli-web/node_modules/.pnpm/vite@7.0.5_@types+node@22.16.5_jiti@2.4.2_sass@1.89.2/node_modules/vite/node_modules:/workspace/aicli-web/node_modules/.pnpm/vite@7.0.5_@types+node@22.16.5_jiti@2.4.2_sass@1.89.2/node_modules:/workspace/aicli-web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
